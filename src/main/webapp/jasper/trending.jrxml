<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" 
	name="trending" pageWidth="792" pageHeight="612" orientation="Landscape" 
	whenNoDataType="NoDataSection" columnWidth="752" leftMargin="20" rightMargin="20" 
	topMargin="20" bottomMargin="20">
	<parameter name="appId" class="java.util.List" isForPrompting="false"/>
	<field name="newVulns" class="java.lang.Integer"/>
	<field name="oldVulns" class="java.lang.Integer"/>
	<field name="totVulns" class="java.lang.Integer"/>
	<field name="fixedVulns" class="java.lang.Integer"/>
	<field name="resurfacedVulns" class="java.lang.Integer"/>
	<field name="importTime" class="java.util.Calendar"/>
	<field name="name" class="java.lang.String"/>
	<background>
		<band/>
	</background>
	<title>
		<band/>
	</title>
	<pageHeader>
		<band height="40">
			<staticText>
				<reportElement x="301" y="0" width="150" height="20"/>
				<textElement textAlignment="Center" verticalAlignment="Middle">
					<font fontName="Arial" size="12" isBold="true" pdfFontName="Helvetica-Bold"/>
				</textElement>
				<text><![CDATA[Trending Report]]></text>
			</staticText>
			<textField>
				<reportElement x="222" y="20" width="309" height="20"/>
				<textElement textAlignment="Center" verticalAlignment="Middle">
					<font fontName="Arial" size="12" isBold="true" pdfFontName="Helvetica-Bold"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["Application: " + ($P{appId}.size() > 1 ? "All" : $F{name})]]></textFieldExpression>
			</textField>
		</band>
	</pageHeader>
	<columnHeader>
		<band/>
	</columnHeader>
	<detail>
		<band/>
	</detail>
	<columnFooter>
		<band/>
	</columnFooter>
	<pageFooter>
		<band/>
	</pageFooter>
	<summary>
		<band height="200">
			<timeSeriesChart>
				<chart>
					<reportElement x="0" y="0" width="752" height="200"/>
					<chartTitle/>
					<chartSubtitle/>
					<chartLegend position="Top">
						<font fontName="Arial" isBold="true" pdfFontName="Helvetica-Bold"/>
					</chartLegend>
				</chart>
				<timeSeriesDataset>
					<timeSeries>
						<seriesExpression><![CDATA["New"]]></seriesExpression>
						<timePeriodExpression><![CDATA[$F{importTime}.getTime()]]></timePeriodExpression>
						<valueExpression><![CDATA[$F{newVulns} != null ? $F{newVulns} : new Integer(0)]]></valueExpression>
					</timeSeries>
					<timeSeries>
						<seriesExpression><![CDATA["Total"]]></seriesExpression>
						<timePeriodExpression><![CDATA[$F{importTime}.getTime()]]></timePeriodExpression>
						<valueExpression><![CDATA[$F{totVulns} != null ? $F{totVulns} : new Integer(0)]]></valueExpression>
					</timeSeries>
					<timeSeries>
						<seriesExpression><![CDATA["Fixed"]]></seriesExpression>
						<timePeriodExpression><![CDATA[$F{importTime}.getTime()]]></timePeriodExpression>
						<valueExpression><![CDATA[$F{fixedVulns} != null ? $F{fixedVulns} : new Integer(0)]]></valueExpression>
					</timeSeries>
					<timeSeries>
						<seriesExpression><![CDATA["Resurfaced"]]></seriesExpression>
						<timePeriodExpression><![CDATA[$F{importTime}.getTime()]]></timePeriodExpression>
						<valueExpression><![CDATA[$F{resurfacedVulns} != null ? $F{resurfacedVulns} : new Integer(0)]]></valueExpression>
					</timeSeries>
					<timeSeries>
						<seriesExpression><![CDATA["Old"]]></seriesExpression>
						<timePeriodExpression><![CDATA[$F{importTime}.getTime()]]></timePeriodExpression>
						<valueExpression><![CDATA[$F{oldVulns} != null ? $F{oldVulns} : new Integer(0)]]></valueExpression>
					</timeSeries>
				</timeSeriesDataset>
				<timeSeriesPlot>
					<plot>
						<seriesColor seriesOrder="0" color="#A00B3E"/>
						<seriesColor seriesOrder="1" color="#BF73E8"/>
						<seriesColor seriesOrder="2" color="#E2E617"/>
						<seriesColor seriesOrder="3" color="#B21D46"/>
						<seriesColor seriesOrder="4" color="#ABA72B"/>
					</plot>
					<timeAxisFormat>
						<axisFormat>
							<labelFont/>
							<tickLabelFont/>
						</axisFormat>
					</timeAxisFormat>
					<valueAxisFormat>
						<axisFormat>
							<labelFont/>
							<tickLabelFont/>
						</axisFormat>
					</valueAxisFormat>
				</timeSeriesPlot>
			</timeSeriesChart>
		</band>
	</summary>
	<noData>
		<band height="20">
			<staticText>
				<reportElement x="222" y="0" width="309" height="20"/>
				<textElement textAlignment="Center" verticalAlignment="Middle">
					<font fontName="Arial" size="12" isBold="true" pdfFontName="Helvetica-Bold"/>
				</textElement>
				<text><![CDATA[No Data Found For Trending Report]]></text>
